Sign In
<div class="container">
<%= form_for :user, url: "/sessions", method: "post" do |f| %>
  <div>
    <%= f.text_field :email, placeholder: "Email" %>
  </div>
  <div>
    <%= f.text_field :password, placeholder: "Password" %>
  </div>
  <button><%= f.submit "Sign In" %></button>
<% end %>
</div>
